[SpamFilter]
[KillThreadsFilter]
[ListMailsFilter]
[ArchiveSentMailsFilter]
sent_tag=sent

[Filter.1]
query = from:notifications@github.com
tags = +github

[Filter.2]
query = tag:spam
tags = -unread;-INBOX;-archive;-replied


[FolderNameFilter]
folder_explicit_list = INBOX Drafts archlinux archweb AUR Important Archive
folder_transforms = Drafts:drafts archlinux:archlinux archweb:archweb AUR:aur Important:important Archive:archive

[InboxFilter]

[MailMover]
folders = INBOX
INBOX = 'tag:important':Important 'tag:todo':Todo 'tag:spam':Spam
